{
  "name": "microservice",
  "version": "1.8.0",
  "description": "",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && tsc && tsc -p tsconfig.types.json && cp package.json dist/package.json",
    "postbuild": "tsc-alias -v --dir dist -p tsconfig.json",
    "ts-alias": "tsc-alias -v --dir dist -p tsconfig.json",
    "build-interfaces": "tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --fix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quick6response/number_api_event.git"
  },
  "keywords": [
    "typescript",
    "rabbitmq",
    "types"
  ],
  "author": "quick6response",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4"
  },
  "exports": {
    ".": "./dist/index.js",
    "./share": "./dist/share/index.js",
    "./notification": "./dist/microservice/notification/index.js",
    "./api": "./dist/microservice/api/index.js"
  },
  "typesVersions": {
    "*": {
      "share": ["./dist/share/index.d.ts"],
      "notification": ["./dist/microservice/notification/index.d.ts"],
      "api": ["./dist/microservice/api/index.d.ts"],
      "*": ["./dist/index.d.ts"]
    }
  }
}
